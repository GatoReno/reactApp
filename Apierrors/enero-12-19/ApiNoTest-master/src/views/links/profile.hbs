<div class="container p-4 ">
    <div class="row">
        <div class="col-lg-3 col-sm-6">





            {{#if user.owner}}
            <h1>owner</h1>

            <div class="card hovercard">
                <div class="cardheader">

                </div>
                <div class="avatar">
                    <img alt="" src="http://lorempixel.com/100/100/people/9/">
                </div>
                <div class="info">
                    <div class="title">
                        <a target="_blank" href="https://scripteden.com/">Script Eden</a>
                    </div>
                    <div class="desc">{{user.name}}</div>
                    <div class="desc">Curious developer {{user.id_user}}</div>
                    <div class="desc">Tech geek {{user.pass}}</div>
                </div>
                <div class="bottom">
                    <a class="btn btn-primary btn-twitter btn-sm" href="https://twitter.com/webmaniac">
                        <i class="fa fa-twitter"></i>
                    </a>
                    <a class="btn btn-danger btn-sm" rel="publisher" href="https://plus.google.com/+ahmshahnuralam">
                        <i class="fa fa-google-plus"></i>
                    </a>
                    <a class="btn btn-primary btn-sm" rel="publisher" href="https://plus.google.com/shahnuralam">
                        <i class="fa fa-facebook"></i>
                    </a>
                    <a class="btn btn-warning btn-sm" rel="publisher" href="https://plus.google.com/shahnuralam">
                        <i class="fa fa-behance"></i>
                    </a>
                </div>
            </div>
            {{/if}}






        </div>
    </div>
    {{#if user.admin}}

    <div class="row">
        <h3>Admin</h3>

        <div class="">
            <ul class="nav nav-tabs">
                <li class="active"><a data-toggle="tab" href="#home">Perfil Admin</a></li>
                <li><a data-toggle="tab" href="#menu1">Admin</a></li>
                <li><a data-toggle="tab" href="#menu2">Noticias</a></li>
                <li><a data-toggle="tab" href="#menu3">Proyectos</a></li>
                <li><a data-toggle="tab" href="#menu4">Medallas</a></li>

            </ul>

            <div class="tab-content ">
                <div id="home" class="tab-pane fade in active p3">
                    <div class="card hovercard">
                        <div class="cardheader">

                        </div>
                        <div class="avatar">
                            <img alt="" src="http://lorempixel.com/100/100/people/9/">
                        </div>
                        <div class="info">
                            <div class="title">
                                <a target="_blank" href="https://scripteden.com/">Script Eden</a>
                            </div>
                            <div class="desc">{{user.name}}</div>
                            <div class="desc">Curious developer {{user.id_user}}</div>
                            <div class="desc">Tech geek {{user.pass}}</div>
                        </div>
                        <div class="bottom">
                            <a class="btn btn-primary btn-twitter btn-sm" href="https://twitter.com/webmaniac">
                                <i class="fa fa-twitter"></i>
                            </a>
                            <a class="btn btn-danger btn-sm" rel="publisher" href="https://plus.google.com/+ahmshahnuralam">
                                <i class="fa fa-google-plus"></i>
                            </a>
                            <a class="btn btn-primary btn-sm" rel="publisher" href="https://plus.google.com/shahnuralam">
                                <i class="fa fa-facebook"></i>
                            </a>
                            <a class="btn btn-warning btn-sm" rel="publisher" href="https://plus.google.com/shahnuralam">
                                <i class="fa fa-behance"></i>
                            </a>
                        </div>
                    </div>
                </div>
                <div id="menu1" class="tab-pane fade p3">
                    <h3>Admin Info</h3>
                    <p>Some content in menu 1.</p>
                   {{#each json.admins}}
                    <div class="col-12 col-sm-6  col-md-4 col-lg-3">
                        <div class="card text-center">
                            <a href="">
                                <a href="{{id_user}}" target=".blanck">
                                    <h3 class="card-title text-uppercase">
                                        {{name}}
                                    </h3>
                                </a>
                            </a>
                            <div class="card-body ">
                                <div>
                                    {{pass}}
                                </div>
                                <div>
                                    {{url}}
                                </div>
                                {{#each json.links}}
                                <div class="col-12 col-sm-6  col-md-4 col-lg-3">
                                    <div class="card text-center">
                                        <a href="">
                                            <a href="{{url}}" target=".blanck">
                                                <h3 class="card-title text-uppercase">
                                                    {{name}}
                                                </h3>
                                            </a>
                                        </a>
                                        <div class="card-body ">
                                            <div>
                                                {{title}}
                                            </div>
                                            <div>
                                                id user : {{id_user}}
                                            </div>



                                        </div>
                                        <div class="card-footer">
                                            <div>
                                                <br>
                                                <a href="{{id}}" class="btn btn-default btn-sm">Info</a>


                                            </div>
                                            <div>
                                                <br>
                                                creado el día: {{timeago created_at}}
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                {{else}}
                                <div class="col-12 ">
                                    <div class="card text-center">
                                        <div class="card-body">
                                            <p>No hay proyectos</p>
                                            <a href="/links/add" class="btn btn-default">Añadir proyecto</a>
                                        </div>
                                    </div>
                                </div>

                                {{/each}}


                            </div>
                            <div class="card-footer">
                                <div>
                                    <br>
                                    <a href="/links/delete/{{id}}" class="btn btn-default btn-sm">Delete</a>
                                    <a href="/links/update/{{id}}" class="btn btn-default btn-sm">Update</a>

                                </div>
                                <div>
                                    <br>
                                    creado el día: {{timeago created_at}}
                                </div>
                            </div>
                        </div>
                    </div>
                    {{else}}
                    <div class="col-12 ">
                        <div class="card text-center">
                            <div class="card-body">
                                <p>No hay proyectos</p>
                                <a href="/links/add" class="btn btn-default">Añadir proyecto</a>
                            </div>
                        </div>
                    </div>

                    {{/each}}
                    {{! termina menu 1}}
                </div>
                <div id="menu2" class="tab-pane fade p3">
                    <h3>Noticias</h3>
                    <p>Some content in menu 2.</p>


                    {{! termina menu 2}}
                </div>
                <div id="menu3" class="tab-pane fade p3">
                    <h3>Proyectos</h3>
                    <p>Some content in menu 1.</p>
                    {{#each json.links}}
                    <div class="col-12 col-sm-6  col-md-4 col-lg-3">
                        <div class="card text-center">
                            <a href="">
                                <a href="{{url}}" target=".blanck">
                                    <h3 class="card-title text-uppercase">
                                        {{title}}
                                    </h3>
                                </a>
                            </a>
                            <div class="card-body ">
                                <div>
                                    {{description}}
                                </div>
                                <div>
                                    {{url}}
                                </div>
                                {{#each json.links}}
                                <div class="col-12 col-sm-6  col-md-4 col-lg-3">
                                    <div class="card text-center">
                                        <a href="">
                                            <a href="{{url}}" target=".blanck">
                                                <h3 class="card-title text-uppercase">
                                                    {{name}}
                                                </h3>
                                            </a>
                                        </a>
                                        <div class="card-body ">
                                            <div>
                                                {{title}}
                                            </div>
                                            <div>
                                                id user : {{id_user}}
                                            </div>



                                        </div>
                                        <div class="card-footer">
                                            <div>
                                                <br>
                                                <a href="{{id}}" class="btn btn-default btn-sm">Info</a>


                                            </div>
                                            <div>
                                                <br>
                                                creado el día: {{timeago created_at}}
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                {{else}}
                                <div class="col-12 ">
                                    <div class="card text-center">
                                        <div class="card-body">
                                            <p>No hay proyectos</p>
                                            <a href="/links/add" class="btn btn-default">Añadir proyecto</a>
                                        </div>
                                    </div>
                                </div>

                                {{/each}}


                            </div>
                            <div class="card-footer">
                                <div>
                                    <br>
                                    <a href="/links/delete/{{id}}" class="btn btn-default btn-sm">Delete</a>
                                    <a href="/links/update/{{id}}" class="btn btn-default btn-sm">Update</a>

                                </div>
                                <div>
                                    <br>
                                    creado el día: {{timeago created_at}}
                                </div>
                            </div>
                        </div>
                    </div>
                    {{else}}
                    <div class="col-12 ">
                        <div class="card text-center">
                            <div class="card-body">
                                <p>No hay sub-administradores</p>
                                <a href="/links/add" class="btn btn-default sm">Agregar uno</a>
                                <a href="/links/add" class="btn btn-default">Añadir proyecto</a>
                            </div>
                        </div>
                    </div>

                    {{/each}}
                    {{! termina menu 3}}
                </div>

                <div id="menu4" class="tab-pane fade p3">
                    <h3>Medallas</h3>
                    <p>Some content in menu 2.</p>


                    {{! termina menu 4}}
                </div>

            </div>
        </div>



        {{/if}}

    </div>